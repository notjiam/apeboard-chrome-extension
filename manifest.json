{
  "manifest_version": 2,
  "name": "Apeboard Chrome Extension",
  "version": "1.0.0",
  "description": "Chrome extension wrap https://apeboard.finance/ in iframe and get latest price.",
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts":["background.js"],
    "persistent": true
  },
  "author": "NotJiam",
  "permissions": [],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}